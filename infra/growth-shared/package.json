  "version": "0.1.0",
  "engines": {
    "node": "14.x"
  },
  "author": "Origin Protocol Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OriginProtocol/origin/issues"
  },
  "dependencies": {
    "pg": "8.6.0",
    "sequelize": "6.6.2",
    "sequelize-cli": "6.2.0",
    "web3": "1.0.0-beta.34"
